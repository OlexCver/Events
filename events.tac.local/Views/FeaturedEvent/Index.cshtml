@model events.tac.local.Models.FeaturedEvents

<div class="col-sm-4">
    <div class="thumbnail well @Model.CssClass">
        <a href="Event-Detail.html">
            @Model.EventImage
        </a>
        <div class="caption">
            <h3 class="teaser-heading">@Model.Heading</h3>
            <p>
                @Model.Intro
            </p>
            <a href="@Model.Url" class="btn btn-default">
                 @if (string.IsNullOrEmpty(Model.ButtonText)){
                    @:Read more...
                 }
                 else
                 {
                     @Model.ButtonText
                 }
            </a>
        </div>
    </div>
</div>


